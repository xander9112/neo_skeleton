{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dart",
			"command": "dart",
			"args": [
				"run",
				"build_runner",
				"build"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": "build",
			"label": "dart run build_runner build",
			"detail": "{{name.PascalCase()}}"
		},
		{
			"type": "dart",
			"command": "dart",
			"args": [
				"run",
				"build_runner",
				"build",
				"--delete-conflicting-outputs"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": "build",
			"label": "dart run build_runner build --delete-conflicting-outputs",
			"detail": "{{name.PascalCase()}}"
		},
		{
			"type": "dart",
			"command": "dart",
			"args": [
				"fix",
				"--apply"
			],
			"group": "build",
			"label": "dart fix --apply",
			"detail": "{{name.PascalCase()}}"
		}
	]
}