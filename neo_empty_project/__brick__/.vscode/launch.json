    "version": "0.2.0",
    "configurations": [
        {
            "name": "{{appName.pascalCase()}} (Dev)",
            "request": "launch",
            "type": "dart",
            "program": "apps/{{appName.snakeCase()}}/lib/main_development.dart",
            "args": [
                "--flavor",
                "development",
                "--target",
                "lib/main_development.dart"
            ]
        },
        {
            "name": "{{appName.pascalCase()}} (Stage)",
            "request": "launch",
            "type": "dart",
            "program": "apps/{{appName.snakeCase()}}/lib/main_staging.dart",
            "args": [
                "--flavor",
                "staging",
                "--target",
                "lib/main_staging.dart"
            ]
        },
        {
            "name": "{{appName.pascalCase()}}",
            "request": "launch",
            "type": "dart",
            "program": "apps/{{appName.snakeCase()}}/lib/main_production.dart",
            "args": [
                "--flavor",
                "production",
                "--target",
                "lib/main_production.dart"
            ]
        }
    ]
}