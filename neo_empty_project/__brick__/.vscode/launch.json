{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Skeleton (Dev)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "dev",
                "--dart-define-from-file",
                "./env/.dev.json"
            ]
        },
        {
            "name": "Skeleton",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "--dart-define-from-file",
                "./env/.prod.json"
            ]
        },
        {
            "name": "Skeleton : Web (Dev)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define-from-file",
                "./env/.dev.json",
                "-d",
                "chrome",
                "--web-port",
                "3000"
            ]
        },
        {
            "name": "Skeleton : Web",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "--dart-define-from-file",
                "./env/.prod.json",
                "-d",
                "chrome",
                "--web-port",
                "3000"
            ]
        },
        {
            "name": "Skeleton (Dev) Release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--flavor",
                "dev",
                "--dart-define-from-file",
                "./env/.dev.json"
            ]
        },
        {
            "name": "Skeleton (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        }
    ]
}